<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>ActualizaU</servlet-name>
        <servlet-class>Servidor.ActualizaU</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Conector</servlet-name>
        <servlet-class>Servidor.Conector</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Devolucion</servlet-name>
        <servlet-class>Servidor.Devolucion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Cerrar</servlet-name>
        <servlet-class>Servidor.Cerrar</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ActualizaU</servlet-name>
        <url-pattern>/ActualizaU</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Conector</servlet-name>
        <url-pattern>/Conector</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Devolucion</servlet-name>
        <url-pattern>/Devolucion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Cerrar</servlet-name>
        <url-pattern>/Cerrar</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>Inicio</welcome-file>
    </welcome-file-list>
</web-app>
